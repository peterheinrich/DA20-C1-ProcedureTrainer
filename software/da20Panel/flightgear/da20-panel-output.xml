<?xml version="1.0"?>

<PropertyList>
    <generic>
        <output>
            <line_separator>\n</line_separator>
            <var_separator>,</var_separator>
            <binary_mode>false</binary_mode>
            <preamble></preamble>
            <postamble></postamble>
            <chunk>
                <name>Flaps</name>
                <format>%04d</format>
                <node>/surface-positions/flap-pos-norm</node>
                <factor>1000</factor>
            </chunk>
            <chunk>
                <name>Trim</name>
                <format>%02d</format>
                <node>/controls/flight/elevator-trim</node>
                <factor>-10</factor>
                <offset>10</offset>
            </chunk>
        </output>
    </generic>
</PropertyList>