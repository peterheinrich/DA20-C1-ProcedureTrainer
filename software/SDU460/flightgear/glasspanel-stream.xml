<?xml version="1.0"?>

<PropertyList>
  <comment>
    Common instrument data information for glass panel simulation.

    Broadcast (or send to specific IP) via UDP on port 30000 at 10 Hz:

    --generic=socket,out,10,127.0.0.1,30000,udp,glasspanel-stream

  </comment>
  <generic>
    <output>
      <line_separator>newline</line_separator>
      <var_separator>,</var_separator>

      <!-- S5 and SDU460 -->

      <chunk>
        <name>IAS(KTS)</name>
        <type>float</type>
        <format>%.2f</format>
        <node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
      </chunk>
      <chunk>
        <name>TAS(KTS)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/instrumentation/airspeed-indicator/true-speed-kt</node>
      </chunk>
      <chunk>
        <name>GS(KTS)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/velocities/groundspeed-kt</node>
      </chunk>
      <chunk>
        <name>ALT(FT)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/instrumentation/altimeter/indicated-altitude-ft</node>
      </chunk>
      <chunk>
        <name>QNH(HPA)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/instrumentation/altimeter/setting-hpa</node>
      </chunk>
      <chunk>
        <name>PITCH(DEG)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/instrumentation/attitude-indicator/indicated-pitch-deg</node>
      </chunk>
      <chunk>
        <name>ROLL(DEG)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/instrumentation/attitude-indicator/indicated-roll-deg</node>
      </chunk>
      <chunk>
        <name>OAT(DEGC)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/environment/temperature-degc</node>
      </chunk>
      <chunk>
        <name>SLIP_SKID</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/instrumentation/slip-skid-ball/indicated-slip-skid</node>
      </chunk>
      <chunk>
        <name>HDG(deg)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/instrumentation/heading-indicator/indicated-heading-deg</node>
      </chunk>
      
      <!-- SDU460 specific additions -->
      <chunk>
        <name>Track(deg)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/instrumentation/gps/indicated-track-true-deg</node>
      </chunk>
      <chunk>
        <name>LAT</name>
        <type>float</type>
        <format>%.6f</format>
        <node>/position/latitude-deg</node>
      </chunk>
      <chunk>
        <name>LONG</name>
        <type>float</type>
        <format>%.6f</format>
        <node>/position/longitude-deg</node>
      </chunk>
      <chunk>
        <name>RPM</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/engines/engine/rpm</node>
      </chunk>
      <chunk>
        <name>Oil(psi)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/engines/engine/oil-pressure-psi</node>
      </chunk>
      <chunk>
        <name>OilT(f)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/engines/engine/oil-temperature-degf</node>
      </chunk>
      <chunk>
        <name>CHT(f)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/engines/engine/cht-degf</node>
      </chunk>
      <chunk>
        <name>EGT(f)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/engines/engine/egt-degf</node>
      </chunk>
      <chunk>
        <name>Amps</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/systems/electrical/amps</node>
      </chunk>
      <chunk>
        <name>Volts</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/systems/electrical/volts</node>
      </chunk>
      <chunk>
        <name>VS(fpm)</name>
        <type>float</type>
        <format>%.1f</format>
        <node>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</node>
      </chunk>
      <chunk>
        <name>Time</name>
        <type>string</type>
        <format>%s</format>
        <node>/sim/time/gmt-string</node>
      </chunk>

    </output>
  </generic>
</PropertyList>
